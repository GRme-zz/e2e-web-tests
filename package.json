{
  "name": "e2e-web-tests",
  "version": "0.0.1",
  "description": "end to end tests for web frontend",
  "main": "app.js",
  "scripts": {
    "test": "./node_modules/.bin/nightwatch",
    "test-chrome": "./node_modules/.bin/nightwatch --env chrome",
    "npm-test-chrome": "rm -Rf ./node_modules/ && npm install && ./node_modules/.bin/nightwatch --env chrome",
    "test-firefox": "./node_modules/.bin/nightwatch --env firefox",
    "npm-test-firefox": "rm -Rf ./node_modules/ && npm install && ./node_modules/.bin/nightwatch --env firefox",
    "test-safari": "./node_modules/.bin/nightwatch --env safari",
    "test-parallel": "./node_modules/.bin/nightwatch --env chrome,firefox"
  },
  "author": "GRme",
  "license": "ISC",
  "devDependencies": {
    "array-equal": "latest",
    "arraylist": "latest",
    "chai": "latest",
    "chromedriver": "^2.31.0",
    "cucumber": "2.3.1",
    "cucumber-html-report": "latest",
    "cucumber-html-reporter": "^2.0.3",
    "geckodriver": "^1.7.1",
    "intl": "latest",
    "moment": "latest",
    "multiple-cucumber-html-reporter": "^0.2.1",
    "nightwatch": "latest",
    "nightwatch-cucumber": "^7.3.1",
    "phantomjs-prebuilt": "latest",
    "selenium-server-standalone-jar": "^3.5.0",
    "selenium-webdriver": "^3.5.0"
  },
  "dependencies": {
    "assert": "latest",
    "babel-core": "latest",
    "babel-plugin-transform-async-to-generator": "latest",
    "babel-plugin-transform-es2015-modules-commonjs": "latest"
  }
}
